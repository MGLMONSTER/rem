#classBox{
    width: 95%;
    height: auto;
    overflow: hidden;
    margin: 0 auto;
    position: relative;
}
.tabBox{
    width: 100%;
    height: 50px;
    display: flex;
    justify-content: space-around;
    flex-wrap: nowrap;
    box-sizing: border-box;
    border-bottom: 4px solid rgb(38,49,86);
    margin-top: 30px;
}
.tabBox .self{
    width: 160px;
    height: 40px;
    text-align: center;
    line-height: 40px;
    border-radius: 10px;
    background-color: #cccccc;
    border: 1px solid #000000;
}
.tabBox .active{
    background-color: rgb(43,64,115);
    color: #ffffff;
    border: none;
}
